<robot name="robot_manipulator">

	<!-- * * * Link Definitions * * * -->

 	<link name="base_link">
		<visual>
		    <origin xyz="0 0 0.075" rpy="0 0 0"/>
			<geometry>
				<cylinder radius="0.3" length="0.15"/>
			</geometry>
			<material name="Black">
	       		<color rgba="0 0 0 1.0"/>
	     	</material>
		</visual>	
	</link>


	<link name="column_link">
		<visual>
		    <origin xyz="0 0 0.2" rpy="0 0 0"/>
			<geometry>
				<cylinder radius="0.1" length="0.4"/>
			</geometry>
			<material name="Gray">
	       		<color rgba="0.75 0.75 0.75 1.0"/>
	     	</material>
		</visual>	
	</link>
	<link name="joint_manipullator_link">
		<visual>
		    <origin xyz="0 0 0" rpy="1.57 0 0"/>
			<geometry>
				<cylinder radius="0.1" length="0.225"/>
			</geometry>
			<material name="Gray1">
	       		<color rgba="0.5 0.5 0.5 1.0"/>
	     	</material>
		</visual>	
	</link>
	<link name="manipullator_link">
		<visual>
		    <origin xyz="0 0 0.2" rpy="0 0 0"/>
			<geometry>
				<cylinder radius="0.1" length="0.4"/>
			</geometry>
			<material name="Gray">
	       		<color rgba="0.75 0.75 0.75 1.0"/>
	     	</material>
		</visual>	
	</link>
	

	<link name="first_arm_circ1_link">
		<visual>
		    <origin xyz="0 0 0.05" rpy="0 0 0"/>
			<geometry>
				<cylinder radius="0.1" length="0.1"/>
			</geometry>
			<material name="Cyan">
	       		<color rgba="0 0.9 0.9 1.0"/>
	     	</material>
		</visual>	
	</link>
	<link name="first_arm_link">
		<visual>
		    <origin xyz="0.225 0 0.05" rpy="0 0 0"/>
			<geometry>
				<box size="0.45 0.2 0.1"/>
			</geometry>
			<material name="Cyan">
	       		<color rgba="0 0.9 0.9 1.0"/>
	     	</material>
		</visual>	
	</link>
	<link name="first_arm_circ2_link">
		<visual>
		    <origin xyz="0 0 0.05" rpy="0 0 0"/>
			<geometry>
				<cylinder radius="0.1" length="0.1"/>
			</geometry>
			<material name="Cyan">
	       		<color rgba="0 0.9 0.9 1.0"/>
	     	</material>
		</visual>	
	</link>

	<link name="second_arm_circ1_link">
		<visual>
		    <origin xyz="0 0 0.05" rpy="0 0 0"/>
			<geometry>
				<cylinder radius="0.1" length="0.1"/>
			</geometry>
			<material name="Purple">
	       		<color rgba="0.5 0 1.0 1.0"/>
	     	</material>
		</visual>	
	</link>
	<link name="second_arm_link">
		<visual>
		    <origin xyz="0.225 0 0.05" rpy="0 0 0"/>
			<geometry>
				<box size="0.45 0.2 0.1"/>
			</geometry>
			<material name="Purple">
	       		<color rgba="0.5 0 1.0 1.0"/>
	     	</material>
		</visual>	
	</link>
	<link name="second_arm_circ2_link">
		<visual>
		    <origin xyz="0 0 0.05" rpy="0 0 0"/>
			<geometry>
				<cylinder radius="0.1" length="0.1"/>
			</geometry>
			<material name="Purple">
	       		<color rgba="0.5 0 1.0 1.0"/>
	     	</material>
		</visual>	
	</link>

	<link name="third_arm_circ1_link">
		<visual>
		    <origin xyz="0 0 0.05" rpy="0 0 0"/>
			<geometry>
				<cylinder radius="0.1" length="0.1"/>
			</geometry>
			<material name="Yellow">
	       		<color rgba="0.73 1.0 0.2 1.0"/>
	     	</material>
		</visual>	
	</link>
	<link name="third_arm_link">
		<visual>
		    <origin xyz="0.225 0 0.05" rpy="0 0 0"/>
			<geometry>
				<box size="0.45 0.2 0.1"/>
			</geometry>
			<material name="Yellow">
	       		<color rgba="0.73 1.0 0.2 1.0"/>
	     	</material>
		</visual>	
	</link>
	<link name="third_arm_circ2_link">
		<visual>
		    <origin xyz="0 0 0.05" rpy="0 0 0"/>
			<geometry>
				<cylinder radius="0.1" length="0.1"/>
			</geometry>
			<material name="Yellow">
	       		<color rgba="0.73 1.0 0.2 1.0"/>
	     	</material>
		</visual>	
	</link>

	
	<!-- * * * Joint Definitions * * * -->

	<joint name="base_column_joint" type="revolute">
    	<parent link="base_link"/>
    	<child link="column_link"/>
    	<origin xyz="0 0 0.25" rpy="0 0 0"/>
    	<axis xyz="0 0 1"/>
    	<limit lower="-3.1416" upper="3.1416" effort="10" velocity="3"/>
  	</joint>
	<joint name="column_arm_joint" type="revolute">
    	<parent link="column_link"/>
    	<child link="joint_manipullator_link"/>
    	<origin xyz="0 0 0.4" rpy="0 0 0"/>
    	<axis xyz="0 1 0"/>
    	<limit lower="-3.1416" upper="3.1416" effort="10" velocity="3"/>
  	</joint>
	<joint name="manipullator_joint" type="fixed">
    	<parent link="joint_manipullator_link"/>
    	<child link="manipullator_link"/>
    	<origin xyz="0 0 0" rpy="0 0 0"/>
  	</joint>
	
	<joint name="first_arm_circ1_joint" type="revolute">
    	<parent link="base_link"/>
    	<child link="first_arm_circ1_link"/>
    	<origin xyz="0 0 0.15" rpy="0 0 0"/>
    	<axis xyz="0 0 1"/>
    	<limit lower="-3.1416" upper="3.1416" effort="10" velocity="3"/>
  	</joint>
	<joint name="first_arm_joint" type="fixed">
    	<parent link="first_arm_circ1_link"/>
    	<child link="first_arm_link"/>
    	<origin xyz="0.0 0 0" rpy="0 0 0"/>
	</joint>
	<joint name="first_arm_circ2_joint" type="fixed">
    	<parent link="first_arm_link"/>
    	<child link="first_arm_circ2_link"/>
    	<origin xyz="0.45 0 0" rpy="0 0 0"/>
	</joint>

	<joint name="second_arm_circ1_joint" type="revolute">
    	<parent link="first_arm_circ2_link"/>
    	<child link="second_arm_circ1_link"/>
    	<origin xyz="0 0 0.1" rpy="0 0 0"/>
    	<axis xyz="0 0 1"/>
    	<limit lower="-2.67" upper="2.67" effort="10" velocity="3"/>
	</joint>
	<joint name="second_arm_joint" type="fixed">
    	<parent link="second_arm_circ1_link"/>
    	<child link="second_arm_link"/>
    	<origin xyz="0.0 0 0" rpy="0 0 0"/>
	</joint>
	<joint name="second_arm_circ2_joint" type="fixed">
    	<parent link="second_arm_link"/>
    	<child link="second_arm_circ2_link"/>
    	<origin xyz="0.45 0 0" rpy="0 0 0"/>
	</joint>

	<joint name="third_arm_circ1_joint" type="revolute">
    	<parent link="second_arm_circ2_link"/>
    	<child link="third_arm_circ1_link"/>
    	<origin xyz="0 0 -0.1" rpy="0 0 0"/>
    	<axis xyz="0 0 1"/>
    	<limit lower="-2.67" upper="2.67" effort="10" velocity="3"/>
	</joint>
	<joint name="third_arm_joint" type="fixed">
    	<parent link="third_arm_circ1_link"/>
    	<child link="third_arm_link"/>
    	<origin xyz="0.0 0 0" rpy="0 0 0"/>
	</joint>
	<joint name="third_arm_circ2_joint" type="fixed">
    	<parent link="third_arm_link"/>
    	<child link="third_arm_circ2_link"/>
    	<origin xyz="0.45 0 0" rpy="0 0 0"/>
	</joint>

</robot>
